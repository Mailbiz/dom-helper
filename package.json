    "eslint": "^8.11.0",
    "husky": "^7.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.41.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-filesize": "^9.0.2",
